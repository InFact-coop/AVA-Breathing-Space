{
  "name": "avabreathingspace",
  "private": true,
  "version": "1.0.0",
  "description": "_SEO friendly page builder in React.js. Heroes, sign-up forms and calls to action._",
  "main": "package.json",
  "author": "Max Gerber <mgerber94@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity start",
    "start": "sanity start",
    "test": "sanity check",
    "deploy": "sanity graphql deploy && sanity deploy",
    "build": "sanity build public -y",
    "==lint&format==": "-----------------------------------------------------------------------------------",
    "format": "npm run prettier -- --write",
    "lint": "jest --config ../test/jest.lint.js",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "validate": "npm run lint && npm run prettier -- --list-different"
  },
  "keywords": [
    "sanity"
  ],
  "jest-runner-eslint": {
    "cliOptions": {
      "fix": true,
      "ext": [
        ".js",
        ".ts"
      ]
    }
  },
  "dependencies": {
    "@sanity/base": "^1.149.15",
    "@sanity/components": "^1.149.15",
    "@sanity/core": "^1.149.13",
    "@sanity/default-layout": "^1.149.15",
    "@sanity/default-login": "^1.149",
    "@sanity/desk-tool": "^1.149",
    "@sanity/production-preview": "^1.149.0",
    "@sanity/vision": "^1.149.0",
    "prop-types": "^15.6",
    "react": "^16.2",
    "react-dom": "^16.2",
    "react-icons": "^3.10.0"
  },
  "devDependencies": {
    "@sanity/cli": "^1.149.13"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:maxgerber/AVA-Breathing-Space.git"
  }
}
